<table class="table table-hover">
	<thead>
	  <tr>
	    <th>ID</th>
	    <th>Navn</th>
	    <th>Team</th>
	    <th>Telefon</th>
	    <th>Email</th>
	    <th>Make Leader</th>
	    <th colspan="3"></th>
	  </tr>
	</thead>

	<tbody>
	  <% @users.each do |user| %>
	    <tr>
	      <td><%= user.id %></td>
	      <td><%= user.name %></td>
	      <td><%= user.team.name %></td>
	      <td><%= user.phone %></td>
	      <td><%= user.email %></td>
	      <td><% if can? :manage, :all %>
	      	<%= link_to 'Rediger', edit_user_path(user) %>
	      	<% end %>
	      </td>
	    </tr>
	  <% end %>
	</tbody>
</table>